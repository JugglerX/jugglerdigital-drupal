{#
/**
 * @file
 * Theme override for an 'input' #type form element.
 *
 * Available variables:
 * - attributes: A list of HTML attributes for the input element.
 * - children: Optional additional rendered elements.
 *
 * @see template_preprocess_input()
 */

#}
<input data-parsley-multiple="field_tag_location"
       data-parsley-class-handler="#edit-field-tag-location"
       data-parsley-errors-container="#edit-field-tag-location-wrapper"
        {{ attributes | without('data-parsley-multiple') }} />{{ children }}



