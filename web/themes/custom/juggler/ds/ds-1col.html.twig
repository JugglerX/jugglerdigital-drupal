{#
/**
 * @file
 * Display Suite 1 column template.
 *
 * Available variables:
 * - ds_content_wrapper: wrapper around content
 * - attributes: content region attributes
 * - ds_content: content region
 */
#}

{%
  set nid = node.id ? 'node-' ~ node.id
%}
{%
  set classes = [
    'ds-1col',
  ]
%}

<div id={{ node.id ? 'node-' ~ node.id }} {{ attributes.addClass(classes) |without('data-history-node-id')}}>
  <div class="group-middle">
    {{ title_suffix.contextual_links }}
    {{ ds_content }}
  </div>
</div>
